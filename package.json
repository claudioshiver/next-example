{
  "name": "next-example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "analyze": "ANALYZE=true next build",
    "start": "next start",
    "release": "semantic-release --debug",
    "release:local": "semantic-release --debug --no-ci --branch $(git branch | grep \\* | cut -d ' ' -f2-)"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.1",
    "classnames": "^2.3.1",
    "next": "10.2.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "^7.2.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^10.2.2",
    "@types/react": "^17.0.6",
    "autoprefixer": "^10.2.5",
    "postcss": "^8.3.0",
    "postcss-import": "^14.0.2",
    "semantic-release": "^17.4.3",
    "tailwindcss": "^2.1.2",
    "typescript": "^4.2.4"
  },
  "release": {
    "branches": [
      "master",
      {
        "name": "development",
        "prerelease": "dev"
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer"
    ]
  }
}
